{
  "name": "@rarify/js-sdk",
  "description": "@rarify/js-sdk is a TypeScript-based source development kit for Rarify platform.",
  "author": "Rarify Tech",
  "version": "1.2.11",
  "license": "MIT",
  "keywords": ["sdk", "rarify", "opensource"],
  "bugs": "https://github.com/rarifytech/js-sdk/issues",
  "homepage": "https://rarifytech.github.io/js-sdk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarifytech/js-sdk.git"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "browser": "./lib/rarify.js-sdk.min.js",
  "unpkg": "./lib/rarify.js-sdk.min.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "sideEffects": false,
  "gitHooks": {
    "pre-commit": "yarn lint && yarn type-check",
    "pre-push": " yarn rsc && yarn test"
  },
  "scripts": {
    "test": "jest --verbose true",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:esm && yarn build:browser",
    "build:cjs": "babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --config-file ./babel.config.node.json",
    "build:esm": "babel src --out-dir lib/esm --extensions \".ts,.tsx\" --config-file ./babel.config.json",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "build:browser": "node ./esbuild.js",
    "clean": "rm -rf lib",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/index.ts",
    "rsc": "node scripts/release-sanity-check.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/atob-lite": "^2.0.0",
    "@types/btoa-lite": "^1.0.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.178",
    "@types/node": "^15.0.1",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-plugin-root-import": "^6.6.0",
    "chalk": "^4.1.2",
    "esbuild": "^0.11.11",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "node-stdlib-browser": "^1.1.0",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.4.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "atob-lite": "^2.0.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "btoa-lite": "^1.0.0",
    "dayjs": "^1.10.8",
    "form-data": "^4.0.0",
    "jsona": "^1.9.3",
    "lodash": "^4.17.21",
    "openpgp": "^5.0.1",
    "uuid": "^8.3.2"
  }
}
